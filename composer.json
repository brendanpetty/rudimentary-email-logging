{
    "name": "brendanpetty/rudimentary-email-logging",
    "description": "Very simple Laravel package to allow log entries to be sent immediately as an email",
    "type": "library",
    "license": "GPL-v3.0",
    "autoload": {
        "psr-4": {
            "Brendanpetty\\RudimentaryEmailLogging\\": "src"
        }
    },
    "authors": [
        {
            "name": "Totally Web Services",
            "email": "brendan@totally.net.au"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0",
        "monolog/monolog": "^3.5"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Brendanpetty\\RudimentaryEmailLogging\\EmailLoggerServiceProvider"
            ]
        }
    }
}
